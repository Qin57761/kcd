<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper1.car.seriesMapper">

<select id="findseries" resultType="series">
select * from car_series
</select>
<select id="findseriesbyid" resultType="series" parameterType="series">
select * from car_series where id=#{id}
</select>
<select id="findseries_v2" resultType="series">
select * from car_series_v2
</select>
<select id="findseriesbyid_v2" resultType="series" parameterType="series">
select * from car_series_v2 where id=#{id}
</select>
</mapper>