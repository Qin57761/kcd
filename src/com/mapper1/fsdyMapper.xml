<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper1.fsdyMapper">

	    <select id="findfsdybyid" parameterType="fsdy" resultType="fsdy">
        select * from assess_fs
        where id=#{id}
        </select>
         
        
        <select id="findfsdy" parameterType="fsdy" resultType="fsdy">
        select * from assess_fs limit 10      
        </select>
        
        
        
        
 </mapper>