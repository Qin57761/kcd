<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper1.icbc1.statesMapper">

<select id="findstates" resultType="states" parameterType="states">
select * from comm_states where id=#{id}
</select>

<select id="allfindstates" resultType="states" parameterType="states">
select * from comm_states order by chrkey asc;
</select>


</mapper>