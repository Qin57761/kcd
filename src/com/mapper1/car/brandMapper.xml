<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper1.car.brandMapper">
<select id="findbrand" resultType="brand">
select * from car_brand
</select>
<select id="findbrandbyid" resultType="brand" parameterType="brand">
select * from car_brand where id=#{id}
</select>
<select id="findbrandbyname" resultType="brand" parameterType="brand">
select * from car_brand where name=#{name}
</select>
</mapper>